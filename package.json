{
  "name": "bs-common-library",
  "description": "All common tools, utils and etc. for Biswap products",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc && tsconfig-replace-paths -s ./src && npm pack"
  },
  "files":[
    "dist/**"
  ],
  "exports": {
    "/": {
      "import": "./dist/index.js",
      "require": null
    },
    "constants": "./dist/constants"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biswapcom/common-library.git"
  },
  "keywords": [
    "nodejs",
    "mongodb",
    "typescript",
    "rabbitmq"
  ],
  "author": "bs-dev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/biswapcom/common-library/issues"
  },
  "homepage": "https://github.com/biswapcom/common-library#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "moment": "^2.29.4",
    "tsconfig-replace-paths": "0.0.11",
    "web3": "^1.7.5"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.8.2"
  }
}
